[alias]
        hist = !git --no-pager log --pretty=format:'%C(green)%h%C(reset) | %C(reset)%s %C(green){{%an}}%C(reset) %C(bold blue)%d%C(reset)' --graph --date=short -n 7 --decorate-refs-exclude='refs/remotes/' --decorate-refs-exclude='refs/tags/'
        files = !git diff-tree --no-commit-id --name-only -r HEAD dev
        fb = !git diff-tree --no-commit-id --name-only -r HEAD
[oh-my-zsh]
        hide-info = 1
